{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-standard"
  ],
  "jsRules": {},
  "rules": {
    "max-classes-per-file": [
      true,
      4
    ],
    "await-promise": [
      false
    ],
    "member-access": [
      false
    ],
    "object-literal-sort-keys": [
      false
    ],
    "trailing-comma": [
      false
    ]
  },
  "rulesDirectory": []
}
